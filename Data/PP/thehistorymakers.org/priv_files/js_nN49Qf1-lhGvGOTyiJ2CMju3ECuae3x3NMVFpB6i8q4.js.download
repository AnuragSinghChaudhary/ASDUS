function hoverMainNav(){
  jQuery('#block-system-main-menu .menu > li').hover(function(){
    jQuery(this).addClass('active');
    },
    function(){
      jQuery(this).removeClass('active');
    });
}
 function slice_me () {
  
jQuery("#block-views-featured-maker-block .view-content .featured-maker-category span.featured-maker").replaceWith(jQuery("#block-views-featured-maker-block .view-content .featured-maker-category span.featured-maker").text().slice(0,-1));
 }

jQuery(document).ready(function() {
  //hide or show text in keyword search

    jQuery('#views-exposed-form-makers-page #edit-name').val('search by name');

      jQuery('#views-exposed-form-makers-page #edit-name').focus(function() {
          if(jQuery(this).val() == 'search by name'){
              jQuery(this).val("");
          }
      });

      jQuery('#views-exposed-form-makers-page #edit-name').blur(function() {
          if(jQuery(this).val() == ""){
              jQuery(this).val('search by name');
          }
      });
  
    jQuery('#block-thm-blocks-std-search-box .search-form .form-text').val('Search');
    
    jQuery('#block-thm-blocks-std-search-box .search-form .form-text').focus(function() {
        if(jQuery(this).val() == 'Search'){
            jQuery(this).val("");
        }
    });

    jQuery('#block-thm-blocks-std-search-box .search-form .form-text').blur(function() {
        if(jQuery(this).val() == ""){
            jQuery(this).val('Search');
        }
    });
    
    jQuery('#views-exposed-form-biography-search-advanced-new-page .form-select option:first-child').text('Select One');
    
  
  //adding last class on the divs
  jQuery('#block-system-main-menu .menu,#block-views-sub-footer-block-1,#block-views-sub-footer-block-2, #block-views-sub-footer-block,#block-views-sub-footer-block .view-sub-footer').addClass('clearfix');
  
  //drop down menu changing ul class name that is a child of menu class ul
  jQuery('.menu ul.menu').removeClass('menu').addClass('sub-menu');
   
  jQuery('#block-system-main-menu').fadeIn('fast');
  //calling hover function.
  hoverMainNav();
  
  //manipulating term names for the featured maker.
  slice_me();
});
