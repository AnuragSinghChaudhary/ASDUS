            if($whySpectrumCalculator.find("input[name='tv']")[0].checked)
                numberOfCheckboxesSelected++;
            if($whySpectrumCalculator.find("input[name='internet']")[0].checked)
                numberOfCheckboxesSelected++;
            if($whySpectrumCalculator.find("input[name='phone']")[0].checked)
