			var svgdec_flag = false;
			var namevalues = match[0].match(/([^< =,]*)=("[^"]*"|[^,"]*)/ig);
			for(var s = 0; s<namevalues.length; s++) {
				if(namevalues[s] === svgdec_text && svgdec_flag) { } else {
