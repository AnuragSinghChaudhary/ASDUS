function(a,b){
if(b.dateStamp) {
  // create a new javascript Date object based on the timestamp
  var date;
  if(b.dateStamp.length == 10) {
    // eg. dateStamp : "1435705321"
    date = new Date(parseInt(b.dateStamp)*1000);
  }
  else {
    // eg. dateStamp : "1435705435478"
    date = new Date(parseInt(b.dateStamp));
  }
  var day = date.getUTCDate();
  if(day<10) day = "0"+day.toString();
  var month = date.getUTCMonth() + 1;
  if(month<10) month="0"+month.toString();
  var year = date.getUTCFullYear();
  b._formattedDate = [month, day, year].join('/');
}

