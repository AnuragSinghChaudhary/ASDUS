function(a,b){
// if the data layer has a productSubscribed then set in local storage
if (typeof b.productSubscribed != "undefined" && b.productSubscribed instanceof Array) {
  // OLD: we used to store the variable in local storage but now switching to session storage (persists for the visit)
  // localStorage.setItem('productSubscribed', JSON.stringify(b.productSubscribed));
  sessionStorage.setItem('productSubscribed', JSON.stringify(b.productSubscribed)); 
}

// try to get localstorage.  if there is not one then productSubscribed will be undefined (void)
// OLD: we used to store the variable in local storage but now switching to session storage (persists for the visit)
// b.productSubscribed = b.productSubscribed || JSON.parse(localStorage.getItem('productSubscribed')) || void(0);
b.productSubscribed = b.productSubscribed || JSON.parse(sessionStorage.getItem('productSubscribed')) || void(0);


// same logic as always where we try to use the productSubscribed data.
if (b.purchaseStep == "features" || b.purchaseStep == 'checkout' || b.purchaseStep == 'order' || b.purchaseStep == 'gig shop order') {
  if (typeof b.productSubscribed != "undefined" && b.productSubscribed instanceof Array) {
    // create a hash to make it easier to check which products are subscribed
    var tmpSubscribed = {};
    for (var i = 0; i < b.productSubscribed.length; i++) {
      tmpSubscribed[b.productSubscribed[i]] = 1;
    }
    b._productNewPSU = b.productPSU.slice(0);
    b._productUptierPSU = b.productPSU.slice(0);
    b._productTotalNewPSUs = 0;
    b._productTotalUptierPSUs = 0;

    // also check for uptiers, which means looking at the PSUs
    // that have the same prefix (ie. are the same product) but
    // are not identical eg. CHSI_TIER_ESSENTIAL vs CHSI_TIER_PREFERRED
    for (var i = 0; i < b.productID.length; i++) {
      var p = b.productID[i], p_prefix;
      // prefix is first two segments:
      // "CHSI_EQUIPMENT_LEASE_USG" becomes "CHSI_EQUIPMENT"
      try { p_prefix = p.split('_').slice(0,2).join('_') } catch(e) {};
      var is_uptier = false;
      // Check for uptiers first
      for (var j = 0; j < b.productSubscribed.length; j++) {
        var sub = b.productSubscribed[j], sub_prefix;
        try{ sub_prefix = sub.split('_').slice(0,2).join('_') } catch(e) {};
        // check for legacy sub prefix cases. If it is legacy – change sub prefix variable to match p_prefix pattern
        if (sub_prefix == "COX_INTERNET") {sub_prefix = "CHSI_TIER"}
        if (sub_prefix == "COX_TELEVISION") {sub_prefix = "CABLE_PACKAGE"}
        if (sub_prefix == "COX_TELEPHONE") {sub_prefix = "TELEPHONE_PACKAGE"} 
        if (sub_prefix && p_prefix && sub_prefix == p_prefix && p != sub) {
          is_uptier = true;
        }
      }

      if (is_uptier && b.productPSU[i] == '1') {
        b._productUptierPSU[i] = '1';
        b._productNewPSU[i] = '0';
        b._productTotalUptierPSUs++;
      }
      else if (!tmpSubscribed[p] && b.productPSU[i] == '1') {
        b._productUptierPSU[i] = '0';
        b._productNewPSU[i] = '1';
        b._productTotalNewPSUs++;
      }
      else {
        b._productUptierPSU[i] = '0';
        b._productNewPSU[i] = '0';
      }
    }
  }
  else {
    // if productSubscribed is not set, then all PSUs are new
    b._productNewPSU = b.productPSU.slice(0);
    b._productTotalNewPSUs = 0;
    for (var i = 0; i < b._productNewPSU.length; i++) {
      if (b._productNewPSU[i] == '1') {
        b._productTotalNewPSUs++;
      }
    }
  }
}

